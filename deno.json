{
  "name": "@podhmo/with-help",
  "version": "0.2.0",
  "exports": {
    ".": "./src/mod.ts",
    "./parse-args": "./src/parse-args.ts"
  },
  "tasks": {
    "check": "deno check src/**/*.ts examples/**/*.ts",
    "ok": "deno fmt --check && deno lint && deno task check && deno test"
  },
  "publish": {
    "include": ["src/**", "deno.json", "README.md", "LICENSE"],
    "exclude": ["**/*_test.*", "e2e-tests/*.ts"]
  },
  "imports": {
    "@std/cli": "jsr:@std/cli@1",
    "@std/assert": "jsr:@std/assert@1"
  },
  "fmt": {
    "include": [
      "*.json",
      "src/**/*.ts",
      "examples/**/*.ts"
    ],
    "lineWidth": 200
  },
  "lint": {
    "include": [
      "src/**/*.ts",
      "examples/**/*.ts"
    ]
  }
}
